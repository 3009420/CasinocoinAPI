<?xml version="1.0" encoding="UTF-8"?>
<api xmlns="http://ws.apache.org/ns/synapse" name="Casinocoin-ExchangeInfo" context="/CSCExchangeInfo">
    <resource methods="GET">
        <inSequence>
            <clone id="ExecUpdatesParallel">
                <target sequence="updateAlcurEX"/>
                <target sequence="updateAlterdice"/>
            </clone>
            <drop description="Finished"/>
        </inSequence>
        <outSequence/>
        <faultSequence>
            <log description="LogFault">
                <property name="#" value="###### Casinocoin-ExchangeInfo Update Fault #####"/>
                <property name="message" expression="get-property('ERROR_MESSAGE')"/>
            </log>
        </faultSequence>
    </resource>
</api>
