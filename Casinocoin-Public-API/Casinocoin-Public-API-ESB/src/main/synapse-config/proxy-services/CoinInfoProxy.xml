<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse" name="CoinInfoProxy" transports="https http" startOnLoad="true" trace="disable">
    <target>
        <inSequence>
            <property name="OUT_ONLY" value="true" scope="default" type="STRING"/>
            <send>
                <endpoint>
                    <http method="get" uri-template="http://localhost:8280/CSCCoinInfo"/>
                </endpoint>
            </send>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </target>
</proxy>
